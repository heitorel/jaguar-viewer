{
  "name": "jaguar-viewer",
  "displayName": "jaguar-viewer",
  "description": "a extension to display logs generated by jaguar 2",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "main": "./client/out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "plaintext",
        "extensions": [
          ".txt"
        ],
        "aliases": [
          "Plaintext"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "suite": "^0.2.0",
    "vscode-languageclient": "^8.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/mocha": "^10.0.9",
    "@types/node": "^14.0.0",
    "typescript": "^4.0.0",
    "vscode": "^1.1.37"
  }
}
