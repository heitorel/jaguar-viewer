{
  "name": "jaguar-viewer",
  "displayName": "Jaguar Viewer",
  "description": "a extension to display logs generated by jaguar 2",
  "icon": "img/icon.png",
  "version": "0.0.1",
  "publisher": "HeitorBianchi",
	"author": {
		"name": "Heitor Bianchi",
		"email": "heitor.lopes.bianchi@gmail.com"
	},
  "engines": {
    "vscode": "^1.80.0"
  },
  "license": "MIT",
	"homepage": "https://github.com/heitorel/jaguar-viewer",
	"repository": {
		"type": "git",
		"url": "https://github.com/heitorel/jaguar-viewer.git"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"log",
		"jaguar2",
    "jaguar",
		"java",
		"viewer",
		"report",
		"junit"
	],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:csv"
  ],
  "main": "./out/client/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openFile",  
        "title": "Abrir Arquivo Monitorado"
      }
    ],
    "languages": [
      {
        "id": "csv",
        "extensions": [".csv"],
        "aliases": ["CSV Monitor"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "suite": "^0.2.0",
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/mocha": "^10.0.9",
    "@types/node": "^14.0.0",
    "@types/vscode": "^1.94.0",
    "typescript": "^4.8.0",
    "vscode": "^1.70.0",
     "vscode-languageclient": "^7.0.0"
  }
}
